# Public URL of the frontend (accessible from the browser)
NEXT_PUBLIC_URL=https://your-app.example.com

# REST API token for key-value storage (used for in-app notifications)
KV_REST_API_TOKEN=your-kv-rest-api-token

# REST API endpoint for key-value storage (used for in-app notifications)
KV_REST_API_URL=https://your-kv-rest-api-endpoint

# Random secret string to authorize bot actions / sending in-app notifications
KEY=your-secret-key

# API key used to verify/decode webhook payloads for in-app notifications
NEYNAR_API_KEY=your-neynar-api-key

# Private signer key of the bot account (omit the "0x" prefix)
PRIVATE_KEY=your-private-key-without-0x

# Snapchain hub URL
HUB_URL=https://your-snapchain-hub.example.com

# Redis connection URL used for checking if same milestone notifications were sent before
REDIS_URL=redis://:password@host:port/0



